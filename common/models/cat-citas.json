{
  "name": "cat_citas",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fecha_cita": {
      "type": "date",
      "required": true
    },
    "consultorio": {
      "type": "string",
      "required": true
    },
    "estadoPago": {
      "type": "string",
      "required": true,
      "default": "PENDIENTE"
    }
  },
  "validations": [],
  "relations": {
    "clientes": {
      "type": "belongsTo",
      "model": "cat_usuarios",
      "foreignKey": "catUsuariosId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
